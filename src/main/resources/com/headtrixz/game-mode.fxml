<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<VBox fx:controller="com.headtrixz.ui.GameMode" alignment="TOP_CENTER" minHeight="300.0" minWidth="600.0" prefHeight="530.0" prefWidth="800.0" spacing="10.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <StackPane>
         <VBox.margin>
            <Insets bottom="50.0" />
         </VBox.margin>
         <children>
            <Text fx:id="gameType" strokeType="OUTSIDE" strokeWidth="0.0" text="Speel [GAME]">
               <font>
                  <Font size="31.0" />
               </font>
            </Text>
            <Button mnemonicParsing="false" text="Terug" StackPane.alignment="CENTER_LEFT" />
         </children>
      </StackPane>
      <Button mnemonicParsing="false" prefWidth="200.0" text="Tegen AI" />
      <Button fx:id="onlineGame" disable="true" mnemonicParsing="false" prefWidth="200.0" text="Online Spelen" />
      <Button fx:id="tournament" disable="true" mnemonicParsing="false" prefWidth="200.0" text="Tournament Modes" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
